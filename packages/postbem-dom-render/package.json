{
  "name": "postbem-dom-render",
  "version": "1.0.0",
  "description": "Render BEMTree to JS DOM.",
  "keywords": [
    "bem",
    "postbem",
    "render",
    "dom"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "lib",
  "scripts": {
    "build": "webpack --mode production ./lib/index.js --output ./umd/postbem.production.min.js --module-bind js=babel-loader",
    "release": "np",
    "lint": "eslint lib/**/*.js",
    "test": "mocha test/{**/*,*}.test.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "mocha": "^5.1.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "author": "Ruslan Khusnetdinov <ruslankhh@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/postbem/postbem-dom-render#readme",
  "repository": "https://github.com/postbem/postbem-dom-render.git",
  "bugs": "https://github.com/postbem/postbem-dom-render/issues"
}
